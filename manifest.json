{
    "manifest_version": 2,
    "name": "My New Tab Page",
    "description": "",
    "version": "1.0.0",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "myNewTab.html"
    },
    "permissions": ["storage", "file:///*", "tabs"],
    "options_page": "options.html",
    "icons": {
        "16": "./resources/icon16.png",
        "48": "./resources/icon48.png",
        "128": "./resources/icon128.png"
    }
}
