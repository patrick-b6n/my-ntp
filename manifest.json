{
    "manifest_version": 2,
    "name": "My New Tab Page",
    "description": "Overrides the new tab page",
    "version": "0.1",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": ["storage", "file:///*", "tabs"],
    "options_page": "options.html",
    "web_accessible_resources": ["blank.html"]
}
